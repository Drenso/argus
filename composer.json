{
    "type": "project",
    "license": "MIT",
    "repositories": [{
        "type": "composer",
        "url": "https://composer.drenso.dev"
    }],
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-sockets": "*",
        "antishov/doctrine-extensions-bundle": "^1.4",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "drenso/symfony-shared": "^1.7",
        "friendsofsymfony/jsrouting-bundle": "^2.6",
        "jms/serializer-bundle": "^3.6",
        "lcobucci/jwt": "^3.3",
        "sensio/framework-extra-bundle": "^5.5",
        "sensiolabs/security-checker": "^6.0",
        "sentry/sentry-symfony": "^3.5",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/event-dispatcher": "5.1.*",
        "symfony/flex": "^1.8.0",
        "symfony/framework-bundle": "5.1.*",
        "symfony/http-client": "5.1.*",
        "symfony/lock": "5.1.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "5.1.*",
        "symfony/translation": "5.1.*",
        "symfony/twig-bundle": "5.1.*",
        "symfony/validator": "5.1.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "5.1.*"
    },
    "require-dev": {
        "drenso/phan-extensions": "^3.1",
        "symfony/maker-bundle": "^1.19",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/phpunit/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "fos:js-routing:dump --format=json --target=assets/js/_fos_routes.json": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
