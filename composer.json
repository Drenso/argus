{
    "name": "drenso/argus",
    "description": "This application manages our GitLab applications (settings) and keeps track of the project status.",
    "type": "project",
    "license": "MIT",
    "repositories": [{
        "type": "composer",
        "url": "https://composer.drenso.dev"
    }],
    "require": {
        "php": "8.0.*",
        "ext-hash": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-sockets": "*",
        "antishov/doctrine-extensions-bundle": "1.4.3",
        "bobv/irker-utils": "1.0.0",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/collections": "1.6.8",
        "doctrine/doctrine-bundle": "2.3.2",
        "doctrine/doctrine-migrations-bundle": "3.2.1",
        "doctrine/orm": "2.9.6",
        "doctrine/persistence": "2.2.3",
        "drenso/symfony-shared": "1.27.2",
        "friendsofsymfony/jsrouting-bundle": "2.7.0",
        "jms/serializer": "3.16.0",
        "jms/serializer-bundle": "3.10.0",
        "lcobucci/clock": "2.1.0",
        "lcobucci/jwt": "4.1.5",
        "psr/log": "1.1.4",
        "sensio/framework-extra-bundle": "6.2.1",
        "sentry/sentry-symfony": "4.2.4",
        "symfony/config": "5.4.0",
        "symfony/console": "5.4.1",
        "symfony/dependency-injection": "5.4.1",
        "symfony/dotenv": "5.4.0",
        "symfony/event-dispatcher": "5.4.0",
        "symfony/event-dispatcher-contracts": "2.5.0",
        "symfony/flex": "1.17.6",
        "symfony/framework-bundle": "5.4.1",
        "symfony/http-client": "5.4.1",
        "symfony/http-client-contracts": "2.5.0",
        "symfony/http-foundation": "5.4.1",
        "symfony/http-kernel": "5.4.1",
        "symfony/lock": "5.4.1",
        "symfony/messenger": "5.4.0",
        "symfony/monolog-bundle": "3.7.1",
        "symfony/password-hasher": "5.4.0",
        "symfony/property-access": "5.4.0",
        "symfony/proxy-manager-bridge": "5.4.0",
        "symfony/routing": "5.4.0",
        "symfony/security-bundle": "5.4.1",
        "symfony/security-core": "5.4.0",
        "symfony/security-http": "5.4.0",
        "symfony/string": "5.4.0",
        "symfony/translation": "5.4.1",
        "symfony/translation-contracts": "2.5.0",
        "symfony/twig-bundle": "5.4.0",
        "symfony/validator": "5.4.1",
        "symfony/webpack-encore-bundle": "1.13.2",
        "symfony/yaml": "5.4.0"
    },
    "require-dev": {
        "drenso/phan-extensions": "3.5.1",
        "phpunit/phpunit": "9.5.10",
        "symfony/browser-kit": "5.4.0",
        "symfony/css-selector": "5.4.0",
        "symfony/maker-bundle": "1.36.4",
        "symfony/phpunit-bridge": "5.4.0",
        "symfony/stopwatch": "5.4.0",
        "symfony/web-profiler-bundle": "5.4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/phpunit/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "fos:js-routing:dump --format=json --target=assets/js/_fos_routes.json": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    }
}
