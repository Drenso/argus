{
    "type": "project",
    "license": "MIT",
    "repositories": [{
        "type": "composer",
        "url": "https://composer.drenso.dev"
    }],
    "require": {
        "php": "^7.3",
        "ext-hash": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-sockets": "*",
        "antishov/doctrine-extensions-bundle": "1.4.3",
        "bobv/irker-utils": "1.0.0",
        "doctrine/collections": "1.6.7",
        "doctrine/doctrine-bundle": "2.3.2",
        "doctrine/doctrine-migrations-bundle": "3.1.1",
        "doctrine/orm": "2.9.3",
        "doctrine/persistence": "2.2.1",
        "drenso/symfony-shared": "1.24.0",
        "friendsofsymfony/jsrouting-bundle": "2.7.0",
        "jms/serializer": "3.13.0",
        "jms/serializer-bundle": "3.9.1",
        "lcobucci/jwt": "3.4.5",
        "psr/log": "1.1.4",
        "sensio/framework-extra-bundle": "6.1.5",
        "sentry/sentry-symfony": "4.1.4",
        "symfony/config": "5.3.3",
        "symfony/console": "5.3.4",
        "symfony/dependency-injection": "5.3.4",
        "symfony/dotenv": "5.3.4",
        "symfony/event-dispatcher": "5.3.4",
        "symfony/event-dispatcher-contracts": "2.4.0",
        "symfony/flex": "1.13.3",
        "symfony/framework-bundle": "5.3.4",
        "symfony/http-client": "5.3.4",
        "symfony/http-client-contracts": "2.4.0",
        "symfony/http-foundation": "5.3.3",
        "symfony/http-kernel": "5.3.5",
        "symfony/lock": "5.3.4",
        "symfony/monolog-bundle": "3.7.0",
        "symfony/orm-pack": "2.1.0",
        "symfony/password-hasher": "5.3.4",
        "symfony/property-access": "5.3.4",
        "symfony/routing": "5.3.4",
        "symfony/security-bundle": "5.3.4",
        "symfony/security-core": "5.3.4",
        "symfony/security-http": "5.3.4",
        "symfony/string": "5.3.3",
        "symfony/translation": "5.3.4",
        "symfony/translation-contracts": "2.4.0",
        "symfony/twig-bundle": "5.3.3",
        "symfony/validator": "5.3.4",
        "symfony/webpack-encore-bundle": "1.11.2",
        "symfony/yaml": "5.3.4"
    },
    "require-dev": {
        "drenso/phan-extensions": "3.5.1",
        "symfony/maker-bundle": "1.31.1",
        "symfony/profiler-pack": "1.0.5",
        "symfony/test-pack": "1.0.7"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/phpunit/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "fos:js-routing:dump --format=json --target=assets/js/_fos_routes.json": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    }
}
